from matplotlib import pyplot as plt


data = [13, 15, 16, 16, 19, 20, 20, 21, 22, 22, 23, 25, 25, 25, 30, 33,
33, 33, 33, 35, 35, 36, 40, 45, 46, 52, 70]

plt.boxplot(data, vert=True, patch_artist=True, boxprops = dict(facecolor = 'lightgreen'))
plt.title('Boxplot of Given Age Data')
plt.ylabel('Age')
plt.yticks(range(10, 75, 5))
plt.show()